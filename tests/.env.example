# NeuroQuantumDB Test Environment Configuration
# Copy this file to .env and adjust values as needed

# Database Configuration
NEUROQUANTUM_URL=http://localhost:8080
NEUROQUANTUM_API_KEY=

# Logging Configuration
LOG_LEVEL=INFO
PYTHON_LOG_LEVEL=INFO

# Test Data Configuration
TOTAL_EMPLOYEES=800
TOTAL_DEPARTMENTS=25
TOTAL_DOCUMENTS=150000
TOTAL_ACCESS_LOGS=200000
TOTAL_SECURITY_EVENTS=15000
BATCH_SIZE=1000

# Performance Configuration
REQUEST_TIMEOUT=60
MAX_RETRIES=3
CONCURRENT_REQUESTS=10

# Test Execution Configuration
SKIP_DATA_GENERATION=false
SKIP_LOADING=false
SKIP_VERIFICATION=false
ENABLE_RICH_OUTPUT=true
ENABLE_PROGRESS_BARS=true

# Development Configuration
DEBUG_MODE=false
PROFILE_PERFORMANCE=false
SAVE_RAW_RESPONSES=false

# Machine Learning Configuration
ML_MODEL_CACHE_DIR=./models
TRAINING_DATA_SIZE=10000
ENABLE_GPU=false

# Quantum Computing Configuration
QUANTUM_BACKEND=simulator
QUANTUM_SHOTS=1024

# DNA Compression Configuration
DNA_ENCODING_METHOD=base4
DNA_COMPRESSION_LEVEL=9

# Security Configuration
ENABLE_SSL_VERIFICATION=true
API_RATE_LIMIT=100
MAX_FILE_SIZE_MB=100
